{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@customTypes/*": ["src/types/*"],
      "@content/*": ["src/content/*"],
      "@utils/*": ["src/utils/*"],
      "@i18n/*": ["src/i18n/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
